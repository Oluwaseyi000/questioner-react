{
  "name": "questioner-react",
  "version": "1.0.0",
  "description": "Questioner application with react js",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "jest",
    "test-cover": "jest -u | coveralls NODE_ENV=test ",
    "test:dev": "jest --watchAll -u",
    "coveralls": "jest | coveralls",
    "coverage": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "start-dev": "webpack-dev-server --open --hot --mode development",
    "build": "webpack --mode production",
    "precommit": "NODE_ENV=production lint-staged",
    "lint": "./node_modules/.bin/eslint ./src/",
    "lint-fix": "./node_modules/.bin/eslint ./src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Oluwaseyi000/questioner-react.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Oluwaseyi000/questioner-react/issues"
  },
  "homepage": "https://github.com/Oluwaseyi000/questioner-react#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.3.0",
    "coveralls": "^3.0.3",
    "css-loader": "^6.7.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^8.0.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.4.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "@testing-library/react": "^8.0.1",
    "axios": "^0.18.0",
    "express": "^4.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "eslint src/ --fix",
      "git add"
    ]
  }
}
